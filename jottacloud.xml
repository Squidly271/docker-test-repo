<?xml version="1.0"?>
<Container version="2">
  <Name>Jottalib</Name>
  <Repository>rix1337/docker-jottacloud</Repository>
  <Registry>https://registry.hub.docker.com/u/rix1337/docker-jottacloud</Registry>
  <Network>bridge</Network>
  <MyIP/>
  <Privileged>false</Privileged>
  <Support>http://lime-technology.com/forum/index.php?topic=43669.0</Support>
  <Overview>A container to sync files from a folder to Jottacloud.</Overview>
  <Category>Backup: Cloud:</Category>
  <WebUI/>
  <TemplateURL/>
  <Icon>https://i.imgur.com/rhny1Kj.png</Icon>
  <ExtraParams/>
  <PostArgs/>
  <DateInstalled></DateInstalled>
  <Description/>
  <Networking>
    <Mode>bridge</Mode>
    <Publish/>
  </Networking>
  <Data>
    <Volume>
      <HostDir>/mnt/user/Jottacloud</HostDir>
      <ContainerDir>/sync</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <Environment>
    <Variable>
      <Value>yourusername</Value>
      <Name>JOTTACLOUD_USERNAME</Name>
      <Mode/>
    </Variable>
    <Variable>
      <Value>yourpassword</Value>
      <Name>JOTTACLOUD_PASSWORD</Name>
      <Mode/>
    </Variable>
  </Environment>
  <Config Name="Sync folder" Target="/sync" Default="" Mode="rw" Description="Container Path: /sync" Type="Path" Display="always" Required="true" Mask="false">/mnt/user/Jottacloud</Config>
  <Config Name="Username" Target="JOTTACLOUD_USERNAME" Default="" Mode="" Description="Container Variable: JOTTACLOUD_USERNAME" Type="Variable" Display="always" Required="true" Mask="false">yourusername</Config>
  <Config Name="Password" Target="JOTTACLOUD_PASSWORD" Default="" Mode="" Description="Container Variable: JOTTACLOUD_PASSWORD" Type="Variable" Display="always" Required="true" Mask="true">yourpassword</Config>
  <Beta>True</Beta>
</Container>
